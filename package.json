{
    "name": "vscode-kuin-syntax",
    "displayName": "Kuin syntax",
    "description": "",
    "version": "0.4.0",
    "publisher": "konosumi",
    "icon": "images/logo_kuin.png",
    "engines": {
        "vscode": "^1.28.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/konosumi/vscode-kuin-syntax.git"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "kuin",
                "aliases": [
                    "kuin",
                    "kuin"
                ],
                "extensions": [
                    "kn"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "kuin",
                "scopeName": "source.kuin",
                "path": "./syntaxes/kuin.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "kuin",
                "path": "./snippets/snippet.json"
            }
        ]
    }
}
